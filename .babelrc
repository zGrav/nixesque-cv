{
    "presets": [
        "env",
        "stage-0",
        "react",
        "flow"
    ],
    "plugins": [
        "add-module-exports",
        "transform-runtime",
        [ "module-resolver", {
            "root": [ "./" ],
            "alias": {
                "app": "./client",
                "components": "./client/components",
                "constants": "./client/constants",
                "containers": "./client/containers",
                "server": "./server",
                "reducers": "./client/reducers",
                "typedef": "./client/typedef"
            }
        } ]
    ]
}
